<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@android:color/transparent" />
            <stroke android:width="1.0px" android:color="@color/link_highlight_bg" />
            <corners android:radius="6.0px" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@android:color/transparent" />
            <stroke android:width="1.0px" android:color="#30000000" />
            <corners android:radius="6.0px" />
        </shape>
    </item>
</selector>
